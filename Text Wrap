import textwrap

def wrap(string, max_width):
    k = ""
    for i in range(0,len(string)+1,max_width):
        k += string[i:i+max_width] + "\n"
            
    
    return k
            
        
            
    

if __name__ == '__main__':
    string, max_width = input(), int(input())
    result = wrap(string, max_width)
    print(result)
