t = int(input())
for _ in range(t):
    n = int(input())
    while(n>>0):
        if(n%2==0):
            n = n//2
        elif(n==1):
            print("YES")
            break
        else:
            n = 3*n+1
        

# Write your code here
t = int(input())
while t:
    n = int(input())
    if n>=2:
        print('YES')
    else:
        print('NO')
    t -= 1
